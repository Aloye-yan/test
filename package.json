    "vant": "^2.2.13",
    "vue": "^2.6.10",
    "vue-cookies": "^1.5.13",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.3",
    "vue-touch": "^2.0.0-beta.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-plugin-import": "^1.12.2",
    "mint-ui": "^2.2.13",
